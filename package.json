{
  "name": "got-it-driver",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start-clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "build:preview:android": "eas build -p android --profile preview",
    "build:preview:ios": "eas build -p ios --profile preview",
    "build:dev:android": "eas build -p android --profile development",
    "build:dev:ios": "eas build -p ios --profile development",
    "build:prod:android": "eas build -p android --profile production",
    "build:prod:ios": "eas build -p ios --profile production",
    "build:status": "eas build:list",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "submit:status": "eas submit:list",
    "update": "eas update",
    "update:dev": "eas update --branch dev --message 'Dev OTA update'",
    "update:staging": "eas update --branch staging --message 'Staging OTA update'",
    "update:prod": "eas update --branch production --message 'Production OTA update'",
    "update:status": "eas update:list",
    "publish": "expo publish",
    "clean": "expo prebuild --clean",
    "reset-project": "node ./scripts/reset-project.js",
    "clear-metro": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf node_modules/.cache/metro",
    "clear-expo": "rm -rf .expo && rm -rf .expo-shared",
    "clear-node": "rm -rf node_modules && yarn install",
    "clear-android": "rm -rf android/build",
    "clear-ios": "rm -rf ios/build",
    "clear-all": "rm -rf node_modules && rm -rf .expo && rm -rf .expo-shared && rm -rf android/build && rm -rf ios/build && yarn install",
    "expo:login": "expo login",
    "expo:logout": "expo logout",
    "expo:whoami": "expo whoami",
    "eas:login": "eas login",
    "eas:logout": "eas logout",
    "eas:whoami": "eas whoami",
    "eas:project": "eas project:init",
    "lint": "expo lint",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.56.1",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-camera": "^16.1.11",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "^18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-secure-store": "^14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.542.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "^1.20.1",
    "react-native-otp-entry": "^1.8.5",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
